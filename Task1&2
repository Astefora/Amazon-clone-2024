      "source": [
        "# === STEP 1: Import Required Libraries ===\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.cluster import KMeans\n",
        "from sklearn.decomposition import PCA\n",
        "from sklearn.metrics import confusion_matrix, classification_report\n",
        "\n",
        "# === STEP 2: Load CSV from GitHub Raw Link ===\n",
        "url = \"https://raw.githubusercontent.com/Astefora/Task112/main/rivers.csv\"\n",
        "df = pd.read_csv(url, encoding='ISO-8859-1')  # Fix UnicodeDecodeError if needed\n",
        "\n",
        "# === STEP 3: Preview the Data ===\n",
        "print(\"Initial Data Shape:\", df.shape)\n",
        "display(df.head())\n",
        "\n",
        "# === STEP 4: Data Cleaning ===\n",
        "df_clean = df.dropna()  # Remove rows with missing values\n",
        "df_features = df_clean.select_dtypes(include=['float64', 'int64'])  # Keep numeric columns\n",
        "print(\"Features used for clustering:\", df_features.columns.tolist())\n",
        "\n",
        "# === STEP 5: Feature Scaling ===\n",
        "scaler = StandardScaler()\n",
        "X_scaled = scaler.fit_transform(df_features)\n",
        "\n",
        "# === STEP 6: KMeans Clustering ===\n",
        "kmeans = KMeans(n_clusters=2, random_state=42)\n",
        "df_clean['Cluster'] = kmeans.fit_predict(X_scaled)\n",
        "\n",
        "# === STEP 7: PCA for Visualization ===\n",
        "pca = PCA(n_components=2)\n",
        "X_pca = pca.fit_transform(X_scaled)\n",
        "\n",
        "# === STEP 8: Plot the Clusters ===\n",
        "plt.figure(figsize=(8, 6))\n",
        "scatter = plt.scatter(X_pca[:, 0], X_pca[:, 1], c=df_clean['Cluster'], cmap='viridis', s=60)\n",
        "plt.title('River Clusters Based on Plastic Pollution')\n",
        "plt.xlabel('PCA Component 1')\n",
        "plt.ylabel('PCA Component 2')\n",
        "plt.colorbar(scatter, label='Cluster')\n",
        "plt.grid(True)\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# === STEP 9: Feature Importance via PCA ===\n",
        "loadings = pd.DataFrame(pca.components_.T, columns=['PC1', 'PC2'], index=df_features.columns)\n",
        "loadings['abs_PC1'] = loadings['PC1'].abs()\n",
        "important_vars = loadings.sort_values(by='abs_PC1', ascending=False)\n",
        "\n",
        "print(\"\\nTop features driving the clustering (PCA PC1):\")\n",
        "print(important_vars[['PC1', 'PC2']].head())\n",
        "\n",
        "# === STEP 10 (Optional): Compare with Known Labels ===\n",
        "# If your dataset contains a 'High_Pollution' column (0 or 1), compare with clusters\n",
