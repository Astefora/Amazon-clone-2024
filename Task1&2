          "metadata": {}
        }
      ],
      "source": [
        "# 1. Import required libraries\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "from sklearn.metrics import (accuracy_score, precision_score, recall_score,\n",
        "                             classification_report, confusion_matrix,\n",
        "                             roc_auc_score, roc_curve)\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "# 2. Load CSV directly from GitHub (no manual upload)\n",
        "github_url = \"https://raw.githubusercontent.com/Astefora/Task112/main/rivers.csv\"\n",
        "data = pd.read_csv(github_url, encoding='latin1')\n",
        "print(\"Initial Data Shape:\", data.shape)\n",
        "display(data.head())\n",
        "\n",
        "# 3. Drop rows with missing values\n",
        "data = data.dropna()\n",
        "\n",
        "# 4. Clean 'M[E] (metric tons year -1)' column\n",
        "data['M[E] (metric tons year -1)'] = (\n",
        "    data['M[E] (metric tons year -1)']\n",
        "    .astype(str)\n",
        "    .str.replace(\"'\", \"\")\n",
        "    .str.replace(\",\", \"\")\n",
        "    .str.replace(\"%\", \"\")\n",
        ")\n",
        "data['M[E] (metric tons year -1)'] = pd.to_numeric(data['M[E] (metric tons year -1)'], errors='coerce')\n",
        "data = data.dropna(subset=['M[E] (metric tons year -1)'])\n",
        "\n",
        "# 5. Create binary target: plastic_contribution\n",
        "data['plastic_contribution'] = data['M[E] (metric tons year -1)'].apply(lambda x: 0 if x > 6008 else 1)\n",
        "\n",
        "# 6. Show count of low/high plastic contributing rivers\n",
        "print(\"\\nPlastic Contribution Classes (0=High, 1=Low):\")\n",
        "print(data['plastic_contribution'].value_counts())\n",
        "\n",
        "# 7. Bar plot: High vs Low Contribution\n",
        "plt.figure(figsize=(6, 4))\n",
        "sns.countplot(data=data, x='plastic_contribution', palette='Set2')\n",
        "plt.xticks([0, 1], ['High Contribution (0)', 'Low Contribution (1)'])\n",
        "plt.title('Plastic Contribution Class vs Number of Rivers')\n",
        "plt.xlabel('Plastic Contribution Class')\n",
        "plt.ylabel('Number of Rivers')\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# 8. Clean all numeric columns\n",
        "for col in data.columns:\n",
        "    if data[col].dtype == 'object':\n",
        "        data[col] = data[col].str.replace(\"'\", \"\").str.replace(\",\", \"\").str.replace(\"%\", \"\")\n",
        "        data[col] = pd.to_numeric(data[col], errors='coerce')\n",
        "\n",
        "# 9. Prepare X and y\n",
        "X = data.drop(columns=[\n",
        "    'M[E] (metric tons year -1)', 'plastic_contribution',\n",
        "    'River name', 'Country or Administrative area'\n",
        "], errors='ignore')\n",
        "data = data.dropna(subset=X.columns)\n",
        "X = data[X.columns]\n",
        "y = data['plastic_contribution']\n",
        "\n",
        "# 10. Scale features\n",
        "scaler = StandardScaler()\n",
        "X_scaled = scaler.fit_transform(X)\n",
        "\n",
        "# 11. Train-test split\n",
        "X_train, X_test, y_train, y_test = train_test_split(\n",
        "    X_scaled, y, test_size=0.3, random_state=42\n",
        ")\n",
        "\n",
        "# 12. Train model\n",
        "model = RandomForestClassifier(random_state=42)\n",
        "model.fit(X_train, y_train)\n",
        "\n",
        "# 13. Predict\n",
        "y_pred = model.predict(X_test)\n",
        "y_proba = model.predict_proba(X_test)[:, 1]  # Probabilities for ROC\n",
        "\n",
        "# 14. Evaluate\n",
        "print(\"\\nModel Evaluation:\")\n",
        "print(\"Accuracy :\", accuracy_score(y_test, y_pred))\n",
        "print(\"Precision:\", precision_score(y_test, y_pred))\n",
        "print(\"Recall   :\", recall_score(y_test, y_pred))\n",
        "print(\"\\nClassification Report:\\n\", classification_report(y_test, y_pred))\n",
        "\n",
        "# 15. Confusion Matrix\n",
        "cm = confusion_matrix(y_test, y_pred)\n",
        "plt.figure(figsize=(6, 4))\n",
        "sns.heatmap(cm, annot=True, fmt='d', cmap='Blues')\n",
        "plt.title(\"Confusion Matrix\")\n",
        "plt.xlabel(\"Predicted\")\n",
        "plt.ylabel(\"Actual\")\n",
        "plt.show()\n",
        "\n",
        "# 16. AUC / ROC Curve\n",
        "roc_auc = roc_auc_score(y_test, y_proba)\n",
        "fpr, tpr, _ = roc_curve(y_test, y_proba)\n",
        "\n",
        "plt.figure(figsize=(8, 5))\n",
        "plt.plot(fpr, tpr, color='darkorange', label=f\"ROC curve (AUC = {roc_auc:.2f})\")\n",
        "plt.plot([0, 1], [0, 1], color='navy', linestyle='--')\n",
        "plt.xlabel(\"False Positive Rate\")\n",
        "plt.ylabel(\"True Positive Rate\")\n",
        "plt.title(\"ROC Curve\")\n",
        "plt.legend(loc=\"lower right\")\n",
        "plt.grid(True)\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# 17. Feature Importance\n",
        "importances = model.feature_importances_\n",
        "features = X.columns\n",
        "indices = np.argsort(importances)[::-1]\n",
        "\n",
        "print(\"\\nFeature Importances:\")\n",
        "for i in indices:\n",
        "    print(f\"{features[i]}: {importances[i]:.4f}\")\n",
        "\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.title(\"Feature Importances\")\n",
        "sns.barplot(x=importances[indices], y=[features[i] for i in indices], palette=\"viridis\")\n",
        "plt.xlabel(\"Importance\")\n",
        "plt.ylabel(\"Feature\")\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    },
